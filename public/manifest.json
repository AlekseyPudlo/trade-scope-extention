{
  "manifest_version": 3,
  "name": "Trade Scope Extension",
  "version": "0.1.0",
  "description": "Chrome extension scaffold for Trade Scope.",
  "action": {
    "default_title": "Trade Scope"
  },
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "permissions": ["sidePanel"],
  "host_permissions": [],
  "side_panel": {
    "default_path": "src/sidepanel.html"
  },
  "minimum_chrome_version": "114"
}
